<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="KeyPadView">
        <attr name="alph" format="string" />
        <attr name="number" format="string" />
        <attr name="text_size" format="integer" />
        <attr name="tone">
            <enum name="tone_dtmf_0" value="0" />
            <enum name="tone_dtmf_1" value="1" />
            <enum name="tone_dtmf_2" value="2" />
            <enum name="tone_dtmf_3" value="3" />
            <enum name="tone_dtmf_4" value="4" />
            <enum name="tone_dtmf_5" value="5" />
            <enum name="tone_dtmf_6" value="6" />
            <enum name="tone_dtmf_7" value="7" />
            <enum name="tone_dtmf_8" value="8" />
            <enum name="tone_dtmf_9" value="9" />
            <enum name="tone_dtmf_x" value="10" />
            <enum name="tone_dtmf_j" value="11" />
        </attr>
    </declare-styleable>

    <!--扩散圆颜色-->
    <attr name="diffuse_color" format="color"/>
    <!--中心圆颜色-->
    <attr name="diffuse_coreColor" format="color"/>
    <!--中心圆图片-->
    <attr name="diffuse_coreImage" format="reference"/>
    <!--中心圆半径-->
    <attr name="diffuse_coreRadius" format="float"/>
    <!--扩散圆宽度,值越小越宽-->
    <attr name="diffuse_width" format="integer"/>
    <!--最大扩散宽度-->
    <attr name="diffuse_maxWidth" format="integer"/>

    <declare-styleable name="DiffuseView">
        <attr name="diffuse_color"/>
        <attr name="diffuse_coreColor"/>
        <attr name="diffuse_coreImage"/>
        <attr name="diffuse_coreRadius"/>
        <attr name="diffuse_width"/>
        <attr name="diffuse_maxWidth"/>
    </declare-styleable>
</resources>