<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
       xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@color/t9_panel_bg">

        <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/t9_panel_line"/>

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:background="@null"
                android:orientation="horizontal">

            <call.ai.com.callsecretary.widget.KeyPadView
                    android:id="@+id/dialNum1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:alph="1"
                    app:number=""
                    app:tone="tone_dtmf_1"/>

            <View
                    android:layout_width="1px"
                    android:layout_height="match_parent"
                    android:background="@color/t9_panel_line"/>

            <call.ai.com.callsecretary.widget.KeyPadView
                    android:id="@+id/dialNum2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:alph="2"
                    app:number="ABC"
                    app:tone="tone_dtmf_2"/>

            <View
                    android:layout_width="1px"
                    android:layout_height="match_parent"
                    android:background="@color/t9_panel_line"/>

            <call.ai.com.callsecretary.widget.KeyPadView
                    android:id="@+id/dialNum3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:alph="3"
                    app:number="DEF"
                    app:tone="tone_dtmf_3"/>
        </LinearLayout>

        <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/t9_panel_line"/>

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:background="@null"
                android:orientation="horizontal">

            <call.ai.com.callsecretary.widget.KeyPadView
                    android:id="@+id/dialNum4"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:alph="4"
                    app:number="GHI"
                    app:tone="tone_dtmf_4"/>

            <View
                    android:layout_width="1px"
                    android:layout_height="match_parent"
                    android:background="@color/t9_panel_line"/>

            <call.ai.com.callsecretary.widget.KeyPadView
                    android:id="@+id/dialNum5"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:alph="5"
                    app:number="JKL"
                    app:tone="tone_dtmf_5"/>

            <View
                    android:layout_width="1px"
                    android:layout_height="match_parent"
                    android:background="@color/t9_panel_line"/>

            <call.ai.com.callsecretary.widget.KeyPadView
                    android:id="@+id/dialNum6"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:alph="6"
                    app:number="MNO"
                    app:tone="tone_dtmf_6"/>
        </LinearLayout>

        <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/t9_panel_line"/>

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:background="@null"
                android:orientation="horizontal">

            <call.ai.com.callsecretary.widget.KeyPadView
                    android:id="@+id/dialNum7"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:alph="7"
                    app:number="PQRS"
                    app:tone="tone_dtmf_7"/>

            <View
                    android:layout_width="1px"
                    android:layout_height="match_parent"
                    android:background="@color/t9_panel_line"/>

            <call.ai.com.callsecretary.widget.KeyPadView
                    android:id="@+id/dialNum8"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:alph="8"
                    app:number="TUV"
                    app:tone="tone_dtmf_8"/>

            <View
                    android:layout_width="1px"
                    android:layout_height="match_parent"
                    android:background="@color/t9_panel_line"/>

            <call.ai.com.callsecretary.widget.KeyPadView
                    android:id="@+id/dialNum9"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:alph="9"
                    app:number="WXYZ"
                    app:tone="tone_dtmf_9"/>
        </LinearLayout>

        <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/t9_panel_line"/>

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:background="@null"
                android:orientation="horizontal">

            <call.ai.com.callsecretary.widget.KeyPadView
                    android:id="@+id/dialx"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:alph="*"
                    app:number=" , "
                    app:tone="tone_dtmf_x"/>

            <View
                    android:layout_width="1px"
                    android:layout_height="match_parent"
                    android:background="@color/t9_panel_line"/>

            <call.ai.com.callsecretary.widget.KeyPadView
                    android:id="@+id/dialNum0"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:alph="0"
                    app:number=" + "
                    app:tone="tone_dtmf_0"/>

            <View
                    android:layout_width="1px"
                    android:layout_height="match_parent"
                    android:background="@color/t9_panel_line"/>

            <call.ai.com.callsecretary.widget.KeyPadView
                    android:id="@+id/dialj"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:alph="#"
                    app:number="@string/phone_paste"
                    app:tone="tone_dtmf_j"/>
        </LinearLayout>

        <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/t9_panel_line"/>

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:background="@null"
                android:orientation="horizontal">

            <TextView
                    android:id="@+id/tv_t9panel_clear_btn"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:paddingLeft="35dp"
                    android:paddingRight="35dp"
                    android:gravity="center"
                    android:text="Clear"
                    android:textStyle="bold"
                    android:textSize="18sp"
                    android:textColor="@color/color_c1c1c1"/>

            <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

            <ImageButton
                    android:id="@+id/ib_t9panel_dial_btn"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="6dp"
                    android:layout_marginBottom="6dp"
                    android:src="@drawable/ic_call_dial"
                    android:background="@drawable/dial_call_button_round"/>

            <ProgressBar
                    android:id="@+id/progress_bar"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_centerInParent="true"
                    android:indeterminateDrawable="@drawable/call_loading"
                    android:visibility="gone"/>
            </RelativeLayout>

            <ImageButton
                    android:id="@+id/ib_t9panel_delete_btn"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:paddingLeft="35dp"
                    android:paddingRight="35dp"
                    android:background="@null"
                    android:src="@drawable/ic_dial_delete_gray"/>
        </LinearLayout>
    </LinearLayout>

</merge>